# This file defines all module routes (Higher priority routes first)
#
# import these routes in the main app as :
# *     /                       module:cmscore
#
# ~~~~

#Node routing
GET     /node/{uuid}/{<[0-9]+>version}      cmscore.NodeController.nodeVersion
GET     /node/{uuid}/all                    cmscore.NodeController.nodeVersions
GET     /node/{uuid}                        cmscore.NodeController.node
GET     /node                               cmscore.NodeController.nodeList

#the public dir for this module
GET     /cmscore/public/                    staticDir:custom/cmscore/public