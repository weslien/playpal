# This file defines all module routes (Higher priority routes first)
#
# import these routes in the main app as :
# *     /                           module:cmscore
#
# ~~~~

#Leaf routing
GET     /leaf/{uuid}/{<[0-9]+>version}      cmscore.Core.leafVersion
GET     /leaf/{uuid}/all                    cmscore.Core.leafVersions
GET     /leaf/{uuid}                        cmscore.Core.leaf
GET     /leaf                               cmscore.Core.leafList
