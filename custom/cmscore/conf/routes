# This file defines all module routes (Higher priority routes first)
#
# import these routes in the main app as :
# *     /                       module:cmscore
#
# ~~~~

#Leaf routing
GET     /leaf/{uuid}/{<[0-9]+>version}      cmscore..LeafController.leafVersion
GET     /leaf/{uuid}/all                    cmscore.LeafController.leafVersions
GET     /leaf/{uuid}                        cmscore.LeafController.leaf
GET     /leaf                               cmscore.LeafController.leafList

#Core routing
GET     /core/{uuid}                        cmscore.CoreController.index


